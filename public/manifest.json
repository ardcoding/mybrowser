{
  "manifest_version": 2,
  "name": "Yeni Sekme Eklentisi",
  "version": "1.0",
  "description": "Tarayıcıda yeni sekme açma eklentisi",
  "permissions": [
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.jpg"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}